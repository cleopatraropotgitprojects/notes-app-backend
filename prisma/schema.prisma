datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Note {
  id          String   @id @default(cuid())
  title       String
  description String
  tags        String[] // tip array text
  createdAt   DateTime @default(now())
  location    String? // optional
}
